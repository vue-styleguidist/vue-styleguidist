    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "semver": "^5.5.0",
    "strip-shebang": "^1.0.2",
    "url-loader": "^1.0.1",
    "vue": "^2.4.2",
    "vuex": "^3.0.0",
    "vue-webpack-loaders": "^1.0.8",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "npm run test:jest",
    "posttest": "npm run format",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "node bin/styleguidist.js server --config examples/basic/styleguide.config.js",
    "start:vuex": "node bin/styleguidist.js server --config examples/vuex/styleguide.config.js",
    "start:vuetify": "node bin/styleguidist.js server --config examples/vuetify/styleguide.config.js",
    "start:nuxtjs": "node bin/styleguidist.js server --config examples/nuxtjs/styleguide.config.js",
    "start:navigation": "node bin/styleguidist.js server --config examples/navigation/styleguide.config.js",
    "start:customised": "node bin/styleguidist.js server --config examples/customised/styleguide.config.js",
    "start:sections": "node bin/styleguidist.js server --config examples/sections/styleguide.config.js",
    "lint": "eslint . --cache --fix",
    "precompile": "rimraf lib/",
    "compile": "babel --ignore '*.spec.js' -d lib/ src/ --plugins=transform-class-properties,transform-object-rest-spread --presets=env,react",
    "compile:watch": "babel --ignore '*.spec.js' --watch -d lib/ src/ --plugins=transform-class-properties,transform-object-rest-spread --presets=env,react",
    "prepublishOnly": "npm run compile",
    "build:basic": "node bin/styleguidist.js build --config examples/basic/styleguide.config.js",
    "build:vuex": "node bin/styleguidist.js build --config examples/vuex/styleguide.config.js",
    "build:nuxtjs": "node bin/styleguidist.js build --config examples/nuxtjs/styleguide.config.js",
    "build:sections": "node bin/styleguidist.js build --config examples/sections/styleguide.config.js",
    "build:navigation": "node bin/styleguidist.js build --config examples/navigation/styleguide.config.js",
    "build:customised": "node bin/styleguidist.js build --config examples/customised/styleguide.config.js",
    "test:browser:pre": "npm i --no-save puppeteer",
    "test:browser:basic": "node test/browser.js examples/basic/styleguide/index.html",
    "test:browser:customised": "node test/browser.js examples/customised/styleguide/index.html",
    "test:browser:sections": "node test/browser.js examples/sections/styleguide/index.html",
    "precommit": "lint-staged",
    "format": "prettier --write '**/*.{js,md}'",
    "test:cypress:pre": "npm i --no-save cypress wait-on",
    "test:cypress:open": "cypress open",
    "test:cypress:run": "cypress run",
    "test:cypress:startServer": "node test/run.server.js",
    "test:cypress:startServer:post": "wait-on http://localhost:8082",
    "release": "node release.js && npm run build:basic"
  },
  "jest": {
    "setupFiles": [
      "./test/raf-polyfill.js",
      "./test/jestsetup.js"
    ],
    "modulePaths": [
      "./src"
    ],
    "moduleNameMapper": {
      "^.+\\.css$": "<rootDir>/test/empty.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/examples/",
      "<rootDir>/test/",
      "<rootDir>/src/rsg-components"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/scripts/server.js",
      "<rootDir>/scripts/build.js"
    ],
    "snapshotSerializers": [
      "deabsdeep/serializer",
      "enzyme-to-json/serializer",
      "jest-serializer-html"
    ]
  },
  "keywords": [
    "vue",
    "styleguide",
    "style guide",
    "documentation",
    "docs",
    "generator",
    "component",
    "components"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  }
}