    "is-directory": "^0.3.1",
    "javascript-stringify": "^1.6.0",
    "jss": "^9.8.1",
    "jss-camel-case": "^6.1.0",
    "jss-compose": "^5.0.0",
    "jss-default-unit": "^8.0.2",
    "jss-global": "^3.0.0",
    "jss-isolate": "^5.1.0",
    "jss-nested": "^6.0.1",
    "leven": "^2.1.0",
    "listify": "^1.0.0",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.5",
    "lowercase-keys": "^1.0.1",
    "markdown-to-jsx": "^6.6.0",
    "mini-html-webpack-plugin": "^0.2.3",
    "minimist": "^1.2.0",
    "opn": "^5.1.0",
    "ora": "^2.0.0",
    "prop-types": "^15.6.1",
    "q-i": "^2.0.1",
    "react": "^16.2.0",
    "react-codemirror2": "^4.2.1",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-group": "^1.0.5",
    "react-icons": "^2.2.7",
    "remark": "^9.0.0",
    "strip-comments": "^0.4.4",
    "style-loader": "^0.20.3",
    "to-ast": "^1.0.0",
    "type-detect": "^4.0.8",
    "uglifyjs-webpack-plugin": "1.2.4",
    "unist-util-visit": "^1.3.0",
    "vue-docgen-api": "^2.3.11",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.2"
