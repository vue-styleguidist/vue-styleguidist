{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "homepage": "https://github.com/vue-styleguidist/vue-styleguidist",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-styleguidist/vue-styleguidist.git"
  },
  "bugs": {
    "url": "https://github.com/vue-styleguidist/vue-styleguidist/issues"
  },
  "scripts": {
    "pretest": "yarn lint",
    "test": "yarn test:jest && yarn test:plugin",
    "test:jest": "jest",
    "test:plugin": "rimraf ./test/cli-packages/* && jest --config=./jest.config.plugin.js --runInBand",
    "posttest": "yarn format",
    "test:coverage": "jest --coverage",
    "start": "node test/styleguidist.js server",
    "start:vuecli3": "vue-cli-service styleguidist --config examples/vuecli3/styleguide.config.js",
    "start:empty": "node bin/styleguidist.js server",
    "lint": "eslint . --cache --fix --ext .ts,.ts --cache-location node_modules/.eslint_cache/",
    "compile": "lerna run compile",
    "build": "node test/styleguidist.js build",
    "build:vuecli3": "vue-cli-service styleguidist:build --config examples/vuecli3/styleguide.config.js",
    "test:browser": "node test/browser.js",
    "precommit": "lint-staged",
    "format": "prettier --write **/*.{js,md,vue,ts}",
    "test:cypress:pre": "yarn add -D -W cypress@3.1.4 wait-on",
    "test:cypress:open": "cypress open",
    "test:cypress": "node test/run.server.js",
    "docs": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "postinstall": "cd ./packages/vue-cli-plugin-styleguidist && yarn link && cd ../.. && yarn link vue-cli-plugin-styleguidist"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/htmlparser2": "^3.7.31",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/pug": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "@vue/cli": "^3.5.1",
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-plugin-typescript": "^3.5.1",
    "@vue/cli-service": "^3.5.1",
    "@znck/prop-types": "^0.5.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-system-import-transformer": "^3.1.0",
    "cheerio": "^1.0.0-rc.2",
    "css-loader": "^2.1.0",
    "deabsdeep": "^1.0.6",
    "deepfreeze": "^2.0.0",
    "dog-names": "^1.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.14.1",
    "eslint-config-tamia": "^6.0.0",
    "eslint-plugin-compat": "^2.4.0",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^1.1.11",
    "inquirer": "^5.2.0",
    "jest": "^24.1.0",
    "jest-serializer-html": "^5.0.0",
    "keymirror": "^0.1.1",
    "lerna": "^3.13.1",
    "lint-staged": "^7.2.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.3",
    "prettier": "1.13.7",
    "prop-types": "^15.6.2",
    "puppeteer": "^1.12.2",
    "qss": "^2.0.3",
    "raf": "^3.4.0",
    "react": "^16.4.1",
    "react-dom": "^16.8.3",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "semver": "^5.5.0",
    "strip-shebang": "^1.0.2",
    "ts-jest": "^24.0.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.3.3333",
    "url-loader": "^1.0.1",
    "vue": "^2.6.9",
    "vue-cli-plugin-styleguidist": "*",
    "vue-i18n": "^8.7.0",
    "vue-loader": "^15.7.0",
    "vue-property-decorator": "^7.3.0",
    "vue-template-compiler": "^2.6.9",
    "vue-types": "^1.3.4",
    "vuepress": "^0.14.10",
    "vuetify": "^1.4.3",
    "vuex": "^3.0.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^2.0.13",
    "yorkie": "^2.0.0"
  },
  "resolutions": {
    "webpack-dev-middleware": "3.6.0"
  },
  "keywords": [
    "vue",
    "styleguide",
    "style guide",
    "documentation",
    "docs",
    "generator",
    "component",
    "components"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  }
}
