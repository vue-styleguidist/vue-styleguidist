  "scripts": {
    "pretest": "npm run lint",
    "test": "npm run test:jest",
    "posttest": "npm run format",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "node test/styleguidist.js server",
    "start:vuecli3": "vue-cli-service styleguidist --config examples/vuecli3/styleguide.config.js",
    "start:empty": "node bin/styleguidist.js server",
    "lint": "eslint . --cache --fix",
    "precompile": "rimraf lib/",
    "compile": "babel --ignore '*.spec.js' -d lib/ src/",
    "compile:watch": "babel --ignore '*.spec.js' --watch -d lib/ src/",
    "prepublishOnly": "npm run compile",
    "build": "node test/styleguidist.js build",
    "build:vuecli3": "vue-cli-service styleguidist:build --config examples/vuecli3/styleguide.config.js",
    "test:browser:pre": "npm i --no-save puppeteer",
    "test:browser": "node test/browser.js",
    "precommit": "lint-staged",
    "format": "prettier --write '**/*.{js,md,vue}'",
    "test:cypress:pre": "npm i --no-save cypress@3.1.4 wait-on",
    "test:cypress:open": "cypress open",
    "test:cypress": "node test/run.server.js",
    "docs": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "jest": {
    "setupFiles": [
      "./test/raf-polyfill.js",
      "./test/jestsetup.js"
    ],
    "modulePaths": [
      "./src"
    ],
    "moduleNameMapper": {
      "^.+\\.css$": "<rootDir>/test/empty.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/examples/",
      "<rootDir>/test/",
      "<rootDir>/src/rsg-components"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/scripts/server.js",
      "<rootDir>/scripts/build.js"
    ],
    "snapshotSerializers": [
      "deabsdeep/serializer",
      "enzyme-to-json/serializer",
      "jest-serializer-html"
    ]
  },
  "keywords": [
    "vue",
    "styleguide",
    "style guide",
    "documentation",
    "docs",
    "generator",
    "component",
    "components"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  }
}
