		"react-dev-utils": "^5.0.0",
		"react-dom": "^16.2.0",
		"react-group": "^1.0.5",
		"react-icons": "^2.2.7",
		"remark": "^9.0.0",
		"strip-comments": "^0.4.4",
		"style-loader": "^0.20.3",
		"to-ast": "^1.0.0",
