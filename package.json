	"keywords": [
		"vue",
		"styleguide",
		"style guide",
		"documentation",
		"docs",
		"generator",
		"component",
		"components"
	],
	"lint-staged": {
		"(*.js|*.ts)": [
			"eslint --fix",
			"prettier --write",
			"git add"
		],
		"*.md": [
			"prettier --write",
			"git add"
		],
		"packages/vue-styleguidist/src/scripts/schemas/config.ts": [
			"pnpm maketypes",
			"git add ./packages/vue-styleguidist/src/types/StyleGuide.ts"
		]
	},
	"gitHooks": {
		"pre-commit": "lint-staged",
		"commit-msg": "commitlint -E GIT_PARAMS"
	},
	"dependencies": {
		"vue-cli-plugin-styleguidist": "^4.31.1",
		"vue-styleguidist": "^4.31.1"
	}
