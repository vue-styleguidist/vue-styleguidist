    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "vue-loader": "^15.2.4",
    "vue-styleguidist": "^3.10.1",
    "webpack": "^4.10.0"
