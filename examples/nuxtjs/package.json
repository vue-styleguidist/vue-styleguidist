{
  "name": "vue-styleguidist-example-nuxtjs",
  "version": "4.0.0",
  "description": "Use beside Nuxt.js and generate your styleguide besides your main app",
  "keywords": [
    "nuxt",
    "nuxtjs",
    "production ready",
    "scaffolding"
  ],
  "author": "Knovour <knovour@gmail.com>",
  "private": true,
  "scripts": {
