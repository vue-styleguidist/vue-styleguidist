{
	"name": "vue-styleguidist-example-vuecli3",
	"version": "4.0.0",
	"description": "When using vue cli 3, prefer using the plugin using `vue add styleguidist`",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"styleguide": "vue-cli-service styleguidist",
		"styleguide:build": "cross-env NODE_ENV=production vue-cli-service styleguidist:build"
	},
	"dependencies": {
		"vue": "^2.6.14"
	},
	"devDependencies": {
		"@vue/babel-preset-app": "^4.5.12",
		"@vue/cli-plugin-babel": "^4.5.12",
		"@vue/cli-plugin-eslint": "^4.5.12",
		"@vue/cli-service": "^4.5.12",
		"vue-cli-plugin-styleguidist": "^4.36.1",
		"vue-template-compiler": "^2.6.14",
		"webpack-bundle-analyzer": "^3.9.0",
		"cross-env": "^7.0.3"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	]
}
