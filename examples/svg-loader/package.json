{
  "name": "vue-styleguidist-example-svg-loader",
  "version": "4.0.0",
  "description": "Here is an example of what can be done using an extenal loader. Observe that the loader added to the cli is automatically added to styleguidist.",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build"
  },
  "dependencies": {
    "vue": "^2.7.11"
  },
  "devDependencies": {
    "@vue/babel-preset-app": "^5.0.8",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "autoprefixer": "^9.8.6",
    "eslint": "^8",
    "vue-cli-plugin-styleguidist": "^4.36.1",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.7.11",
    "webpack-bundle-analyzer": "^3.9.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}