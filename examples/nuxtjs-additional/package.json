{
  "name": "styleguide",
  "version": "1.6.1",
  "description": "Nuxt.js project",
  "scripts": {
    "dev": "nuxt -c ./config/nuxt.config.js",
    "build": "nuxt build -c ./config/nuxt.config.js",
    "start": "nuxt start -c ./config/nuxt.config.js",
    "generate": "nuxt generate -c ./config/nuxt.config.js",
    "styleguide": "vue-styleguidist server --config ./config/styleguide.config.js",
    "styleguide:build": "vue-styleguidist build --config ./config/styleguide.config.js",
    "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
  },
  "author": "piotr.porzuczek@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@nuxtjs/axios": "^5.1.1",
    "bootstrap-vue": "^2.0.0-rc.2",
    "nuxt": "^1.4.0",
    "jquery": "^3.3.1",
    "nuxt-i18n": "^2.9.1",
    "vue-awesome": "^2.3.5",
    "vue-styleguidist": "1.6.1"
  },
  "devDependencies": {
    "ajv": "^6.4.0",
    "autoprefixer": "^8.1.0",
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.1",
    "babel-polyfill": "^6.26.0",
    "eslint": "^4.19.0",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-vue": "^4.0.0",
    "eventsource-polyfill": "^0.9.6",
    "jshint": "^2.9.5",
    "jshint-loader": "^0.8.4",
    "node-sass": "^4.8.3",
    "nuxt-sass-resources-loader": "^1.1.0",
    "postcss-loader": "^2.1.1",
    "promise.prototype.finally": "^3.1.0",
    "require-extension-hooks": "^0.3.2",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^0.4.1",
    "sass-loader": "^6.0.7",
    "vue": "^2.5.16",
    "vue-i18n": "^7.6.0",
    "vue-loader": "^14.2.2",
    "vue-webpack-loaders": "^1.0.6",
    "vuex": "^3.0.1",
    "webpack": "^3.11.0",
    "webpack-node-externals": "^1.7.2"
  }
